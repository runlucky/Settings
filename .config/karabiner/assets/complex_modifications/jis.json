{
  "title": "ここに独自ルールのタイトルを入力",
  "rules": [
    {
      "description": "HOME",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "home",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "END",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "end",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "plus",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "keypad_plus",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "semicolon",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "minus",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "keypad_hyphen",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "hyphen"
            }
          ]
        }
      ]
    },
    {
      "description": "command + pageup",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "page_up",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_up",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
